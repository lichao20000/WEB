<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="resourceBindMemConfig" extends="struts-default,linkage-default,linkage_security" namespace="/gwms/sysConfig">
		<action name="resourceBindMemConfig" class="ResourceBindMemConfigACT">
			<result name="userInfo">/gwms/sysConfig/resourceBindMemConfigUserList.jsp</result>
			<result name="userDetail">/gwms/sysConfig/resourceBindMemConfigUserDetail.jsp</result>
			<result name="ajax" type="ajax"></result>
		</action>
		<action name="itmsInstTest" class="testItmsInstACT">
			<result name="device">/gwms/sysConfig/deviceInfo.jsp</result>
			<result name="userInst">/gwms/sysConfig/itmsInstUserInfo.jsp</result>
			<result name="userRelease">/gwms/sysConfig/itmsReleaseUserInfo.jsp</result>
			<result name="success">/gwms/sysConfig/itmsInstMessage.jsp</result>
			<result name="init">/gwms/sysConfig/itmsInst.jsp</result>
			<result name="ajax" type="ajax"></result>
		</action>
	</package>
</struts>
